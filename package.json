  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.2.0",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "prisma-binding": "^2.3.16",
    "prisma-client-lib": "^1.34.10"
